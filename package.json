{
	"name": "mmmh",
	"version": "0.0.0",
	"description": "collection of utilities, both useful and not",
	"packageManager": "pnpm@7.0.1",
	"type": "commonjs",
	"main": "index.js",
	"module": "index.js",
	"browser": "index.js",
	"types": "index.d.ts",
	"sideEffects": false,
	"files": [
		"mjs"
	],
	"scripts": {
		"clean": "rm -fv *.js *.js.map *.d.ts *.d.ts.map && rm -f *.js *.js.map *.d.ts *.d.ts.map",
		"build": "pnpm run clean && tsc -p tsconfig.mjs.json",
		"test": "mocha"
	},
	"engines": {
		"node": ">=16"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/autumnblazey/mmmh.git"
	},
	"keywords": [
		"util",
		"weird"
	],
	"author": {
		"name": "Autumn Meadow",
		"url": "https://github.com/autumnblazey"
	},
	"license": "MIT",
	"devDependencies": {
		"@types/mocha": "^9.1.1",
		"@types/node": "^17.0.34",
		"mocha": "^10.0.0",
		"ts-node": "^10.7.0",
		"tslib": "^2.4.0",
		"typescript": "^4.6.4"
	}
}
